
@{
    ViewBag.Title = "RestaurantDetailsDisplay";
}
<h1>View Bag</h1>
<table border="1">
    <tr>
        <td>
            Restaurant Name
        </td>
        <td>
            Restaurant Type
        </td>
        <td>
            Location
        </td>
    </tr>
    @foreach(Restaurant.Entity.RestaurantEntity item in ViewBag.RestaurantDetails)
    {
        <tr>
            <td>
                @item.RestaurantName
            </td>
            <td>
                @item.RestaurantType
            </td>
            <td>
                @item.Location
            </td>
        </tr>
    }
</table>

